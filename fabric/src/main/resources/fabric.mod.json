{
	"schemaVersion": 1,
	"id": "efallingtrees",
	"version": "${version}",
	"name": "Enhanced Falling Trees",
	"description": "Enhanced Falling Trees is a mod that introduces realistic falling mechanics to trees, bamboos, and cacti, making your game more immersive with naturalistic plants behavior.\n\n§6Features:§r\n- Realistic tree falling animations\n- Support for bamboo and cacti\n- Customizable settings\n- Performance friendly\n- Works with most tree-generating mods\n",
	"authors": ["addavriance"],
	"contact": {
		"homepage": "https://modrinth.com/mod/enhanced-falling-trees",
		"sources": "https://github.com/addavriance/EnhancedFallingTrees",
		"issues": "https://github.com/addavriance/EnhancedFallingTrees/issues"
	},
	"license": "MIT",
	"icon": "icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.adda.enhanced_falling_trees.fabric.FallingTreesFabric"
		],
		"client": [
			"me.adda.enhanced_falling_trees.fabric.FallingTreesClientFabric"
		],
		"modmenu": [
			"me.adda.enhanced_falling_trees.fabric.ModMenuIntegration"
		],
		"jade": [
			"me.adda.enhanced_falling_trees.compat.Jade"
		]
	},
	"mixins": [
		"efallingtrees.mixins.json",
		"efallingtrees-common.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": ">=1.20",
		"fabric-api": ">=0.83.0",
		"cloth-config": ">=11.1.106",
		"architectury": ">=9.1.12"
	},
	"suggests": {
		"modmenu": ">=7.0.1"
	},
	"custom": {
